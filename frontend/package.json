{
  "name": "toddagriscience frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "overrides": {
    "glob": "^11.1.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "bun build-api-routes && next build",
    "build-api-routes": "node ./src/scripts/create-env-local.mjs && node --env-file=.env.local ./src/scripts/build-api-routes.mjs && prettier --write ./src/apiRoutes.json",
    "start": "next start",
    "lint": "eslint .",
    "prelint:fix": "tsx ./scripts/prep-for-license.ts",
    "lint:fix": "bun run prelint:fix && eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "ci": "bun run type-check && bun run lint && bun run test && bun run build",
    "validate": "bun run format:check && bun run ci",
    "generate-icons": "tsx scripts/generate-icons.ts"
  },
  "dependencies": {
    "@portabletext/types": "^4.0.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sanity/image-url": "^2.0.2",
    "@sanity/vision": "^4.21.1",
    "@studio-freight/lenis": "^1.0.42",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.85.0",
    "@types/ua-parser-js": "^0.7.39",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commitlint": "^20.2.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "glob": "11.1.0",
    "jsdom": "^27.3.0",
    "lucide-react": "^0.544.0",
    "next": "^16.0.10",
    "next-intl": "^4.4.0",
    "next-sanity": "^11.6.10",
    "posthog-js": "^1.298.1",
    "react": "^19.2.1",
    "react-docgen": "^8.0.2",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "ua-parser-js": "^2.0.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@storybook/addon-a11y": "^10.0.2",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-links": "^10.0.2",
    "@storybook/addon-onboarding": "^10.0.2",
    "@storybook/nextjs-vite": "^10.0.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "4.0.15",
    "baseline-browser-mapping": "^2.8.32",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "sharp": "^0.34.4",
    "storybook": "^10.0.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^4.0.15",
    "whatwg-fetch": "^3.6.20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md,yml,yaml,html}": [
      "prettier --write"
    ]
  }
}
